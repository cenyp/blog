    "列表页面模版": {
        "prefix": "uf_tablePage",
        "body": [
            "<template>",
            "    <div>",
            "        <div v-show=\"!showDetail\">",
            "            <a-form ref=\"formRef\" :model=\"formState\">",
            "                <a-row :gutter=\"70\">",
            "                    <a-col :span=\"6\">",
            "                        <a-form-item name=\"timeRange\" label=\"上架时间\">",
            "                            <a-range-picker v-model:value=\"formState.timeRange\" value-format=\"YYYY-MM-DD\" />",
            "                        </a-form-item>",
            "                    </a-col>",
            "",
            "                    <a-col :span=\"12\" class=\"right\">",
            "                        <a-button type=\"primary\" @click=\"search\"> 查询 </a-button>",
            "                        <a-button @click=\"reset\"> 重置 </a-button>",
            "                        <a-button type=\"primary\" @click=\"handleLine('add')\" v-permission>",
            "                            新增",
            "                        </a-button>",
            "                    </a-col>",
            "                </a-row>",
            "            </a-form>",
            "",
            "            <lb-table :columns=\"mainColumns\" :api=\"`api`\" :params=\"params\" :scroll=\"{ x: 1300 }\" list=\"records\"",
            "                ref=\"tableRef\" :formatFn=\"formatFn\">",
            "                <template #bodyCell=\"{ column, record }\">",
            "                    <div v-if=\"column.dataIndex === 'operation'\">",
            "                        <a-space>",
            "                            <span><a @click=\"handleLine('read', record)\" v-permission> 查看 </a></span>",
            "                            <span><a @click=\"handleLine('edit', record)\" v-permission> 编辑 </a></span>",
            "                        </a-space>",
            "                    </div>",
            "                </template>",
            "            </lb-table>",
            "        </div>",
            "",
            "        <detail v-if=\"showDetail\" :handleRow=\"handleRow\" :type=\"handleType\" @ok=\"ok\" @cancel=\"cancel\" />",
            "    </div>",
            "</template>",
            "",
            "<script lang=\"ts\" setup>",
            "// =======  依赖引入  =======",
            "import { FormInstance, message, Modal } from 'ant-design-vue'",
            "import type { Dayjs } from 'dayjs'",
            "import { formatTime } from '@/utils/index'",
            "import Detail from './detail.vue'",
            "",
            "// =======  类型声明  =======",
            "interface FormStateType {",
            "    timeRange: Dayjs[]",
            "}",
            "interface RowType {",
            "    id: number",
            "}",
            "// =======  变量声明  =======",
            "const mainColumns = [",
            "    { title: '操作', dataIndex: 'operation', width: 200 },",
            "    { title: 'xxx', dataIndex: 'xxx', width: 200, tooltip: true },",
            "]",
            "const tableRef = ref()",
            "const formRef = ref<FormInstance>()",
            "const params = ref({})",
            "const showDetail = ref(false)",
            "const handleRow = ref({})",
            "const handleType = ref('')",
            "const formState = ref<FormStateType>({",
            "    timeRange: []",
            "})",
            "",
            "// =======  主流程  =======",
            "",
            "// =======  函数声明  =======",
            "function cancel() {",
            "    showDetail.value = false",
            "}",
            "",
            "function ok() {",
            "    showDetail.value = false",
            "    tableRef.value.getCurrentInfo()",
            "}",
            "",
            "function handleLine(type: string, row?: RowType) {",
            "    if (type === 'disable') {",
            "        Modal.confirm({",
            "            title: '温馨提示',",
            "            content: `确定禁用吗？`,",
            "            onOk() {",
            "                xxx({ id: row!.id!, state: 2 }).then(() => {",
            "                    tableRef.value.getCurrentInfo()",
            "                    message.success(`禁用成功`)",
            "                })",
            "            },",
            "        })",
            "        return",
            "    }",
            "",
            "    handleRow.value = row || {}",
            "    handleType.value = type",
            "    showDetail.value = true",
            "",
            "}",
            "",
            "function getParams() {",
            "    const { timeRange } = formState.value",
            "    const { startTime, endTime } = formatTime(timeRange)",
            "    return { startTime, endTime }",
            "}",
            "",
            "function search() {",
            "    params.value = getParams()",
            "    tableRef.value.getTableInfo()",
            "}",
            "",
            "function reset() {",
            "    formRef.value!.resetFields()",
            "    search()",
            "}",
            "",
            "function formatFn(data: []) {",
            "    return data.map((e: RowType) => {",
            "        return {",
            "            ...e,",
            "        }",
            "    })",
            "}",
            "",
            "// =======  属性返回  =======",
            "</script>",
            "",
            "<style lang=\"scss\" scoped>",
            ".right {",
            "    text-align: right;",
            "}",
            "</style>",
            ""
        ],
        "description": "列表页面模版"
    },
    "详情页面模版": {
        "prefix": "uf_detailPage",
        "body": [
            "<template>",
            "    <div class=\"detail\">",
            "        <lb-page-header :title=\"title\" @back=\"back\" />",
            "        <a-form ref=\"formRef\" :model=\"formState\" :label-col=\"{ span: 8 }\" :wrapper-col=\"{ span: 14 }\" :rules=\"rules\">",
            "",
            "            <div class=\"footer\" v-if=\"!isDisabled\">",
            "                <a-button @click=\"back\">取消</a-button>",
            "                <a-button type=\"primary\" @click=\"submit\" :loading=\"btnLoading\"> 确定 </a-button>",
            "            </div>",
            "        </a-form>",
            "    </div>",
            "</template>",
            "",
            "<script lang=\"ts\" setup>",
            "// =======  依赖引入  =======",
            "import type { Rule } from 'ant-design-vue/es/form'",
            "import { FormInstance, Modal, message } from 'ant-design-vue'",
            "import { scrollToError } from '@/utils/form'",
            "",
            "// =======  类型声明  =======",
            "interface FormStateType {",
            "}",
            "",
            "",
            "// =======  变量声明  =======",
            "const props = defineProps({",
            "    handleRow: {",
            "        type: Object,",
            "        required: true,",
            "    },",
            "    type: {",
            "        type: String,",
            "        required: true,",
            "    },",
            "})",
            "const emit = defineEmits<{",
            "    (_event: 'cancel'): void",
            "    (_event: 'ok'): void",
            "}>()",
            "const title = ref('查看')",
            "const isDisabled = ref(false)",
            "const formState = ref<FormStateType>({",
            "",
            "})",
            "",
            "const rules: Record<string, Rule[]> = {",
            "    xxx: [{ required: true, message: 'xxx不能为空' }],",
            "    xxx: [",
            "        {",
            "            required: true,",
            "            validator: (_rule: Rule, value: number[]) => {",
            "                if (value?.length === 0) {",
            "                    return Promise.reject('xxx不能为空')",
            "                } else {",
            "                    return Promise.resolve()",
            "                }",
            "            },",
            "        },",
            "    ],",
            "}",
            "const formRef = ref()",
            "const btnLoading = ref(false)",
            "",
            "// =======  主流程  =======",
            "if (props.type === 'edit') {",
            "    title.value = '编辑'",
            "    isDisabled.value = false",
            "} else if (props.type === 'add') {",
            "    title.value = '新增'",
            "    isDisabled.value = false",
            "} else {",
            "    isDisabled.value = true",
            "}",
            "",
            "// =======  函数声明  =======",
            "function back() {",
            "    emit('cancel')",
            "}",
            "",
            "function submit() {",
            "    formRef.value?.validateFields().then(() => {",
            "        btnLoading.value = true",
            "",
            "        xxx.then(() => {",
            "            message.success('保存成功')",
            "            emit('ok')",
            "        }).finally(() => {",
            "            btnLoading.value = false",
            "        })",
            "    })",
            ".catch(() => {",
            "    scrollToError()",
            "})",
            "}",
            "// =======  属性返回  =======",
            "</script>",
            "",
            "<style lang=\"scss\" scoped>",
            ".detail {",
            "    margin-bottom: 52px;",
            "    .footer {",
            "        display: flex;",
            "        justify-content: flex-end;",
            "        position: fixed;",
            "        bottom: 0;",
            "        left: 240px;",
            "        width: calc(100% - 240px);",
            "        padding: 10px 44px;",
            "        background-color: #fff;",
            "        box-shadow: 4px 0 10px 0 rgba(34, 40, 53, 0.2);",
            "    }",
            "}",
            "</style>",
            ""
        ],
        "description": "详情页面模版"
    },
    "弹窗详情页面模版": {
        "prefix": "uf_modalDetailPage",
        "body": [
            "<template>",
            "    <a-modal v-model:visible=\"modalVisible\" :title=\"title\" :width=\"750\" :mask-closable=\"false\" class=\"detail\">",
            "        <a-form ref=\"formRef\" :model=\"formState\" :label-col=\"{ span: 6 }\" :wrapper-col=\"{ span: 14 }\" :rules=\"rules\">",
            "",
            "        </a-form>",
            "",
            "        <template #footer>",
            "            <template v-if=\"type !== 'read'\">",
            "                <a-button key=\"back\" @click=\"modalVisible = false\">取消</a-button>",
            "                <a-button key=\"submit\" type=\"primary\" :loading=\"btnLoading\" @click=\"handleOk\">确定</a-button>",
            "            </template>",
            "        </template>",
            "    </a-modal>",
            "</template>",
            "",
            "<script lang=\"ts\" setup>",
            "// =======  依赖引入  =======",
            "import type { Rule } from 'ant-design-vue/es/form'",
            "import { FormInstance, message } from 'ant-design-vue'",
            "import { scrollToError } from '@/utils/form'",
            "",
            "// =======  类型声明  =======",
            "interface FormStateType {",
            "",
            "}",
            "",
            "// =======  变量声明  =======",
            "const emit = defineEmits<{",
            "    (_event: 'update:visible', _visible: boolean): void",
            "    (_event: 'ok'): void",
            "}>()",
            "const props = defineProps({",
            "    visible: {",
            "        type: Boolean,",
            "        required: true,",
            "    },",
            "    handleRow: {",
            "        type: Object,",
            "        required: true,",
            "    },",
            "    type: {",
            "        type: String,",
            "        required: true,",
            "    },",
            "})",
            "const modalVisible = computed({",
            "    get() {",
            "        return props.visible",
            "    },",
            "    set(val) {",
            "        emit('update:visible', val)",
            "    },",
            "})",
            "const formState = ref<FormStateType>({",
            "",
            "})",
            "const rules: Record<string, Rule[]> = {",
            "    xxx: [{ required: true, message: 'xxx不能为空' }],",
            "    xxx: [",
            "        {",
            "            required: true,",
            "            validator: async (_rule: Rule, value: string) => {",
            "                if (!value) {",
            "                    return Promise.reject(`xxx不能为空`)",
            "                } else {",
            "                    return Promise.resolve()",
            "                }",
            "            },",
            "        },",
            "    ],",
            "}",
            "const isDisabled = ref(true)",
            "const formRef = ref()",
            "const title = ref('查看')",
            "const btnLoading = ref(false)",
            "",
            "// =======  主流程  =======",
            "if (props.type === 'add') {",
            "    title.value = '新增'",
            "} else if (props.type === 'edit') {",
            "    title.value = '编辑'",
            "} else {",
            "    isDisabled.value = true",
            "}",
            "",
            "// =======  函数声明  =======",
            "function handleOk() {",
            "    formRef.value?.validateFields().then(() => {",
            "        btnLoading.value = true",
            "",
            "        xxx.then(() => {",
            "            message.success('保存成功')",
            "            emit('ok')",
            "        }).finally(() => {",
            "            btnLoading.value = false",
            "        })",
            "    })",
            ".catch(() => {",
            "    scrollToError()",
            "})",
            "}",
            "// =======  属性返回  =======",
            "</script>",
            "",
            "<style lang=\"scss\" scoped></style>",
            "<style lang=\"scss\">",
            ".detail .ant-modal-body {",
            "    overflow-y: auto;",
            "    max-height: 600px;",
            "}",
            "</style>",
            ""
        ],
        "description": "弹窗详情页面模版"
    },
    "获取树数据结构最低级值": {
        "prefix": "uf_getLowestLevelIds",
        "scope": "javascript,typescript",
        "body": [
            "function getLowestLevelIds(treesList: any[][], childrenName: string) {",
            "    let ids: number[] = []",
            "    for (let trees of treesList) {",
            "        for (let tree of trees) {",
            "            if (!tree[childrenName] || tree[childrenName].length === 0) {",
            "                ids.push(tree.id)",
            "            } else {",
            "                ids = ids.concat(getLowestLevelIds([tree[childrenName]], childrenName))",
            "            }",
            "        }",
            "    }",
            "    return ids",
            "}",
            "",
            "function xxx(_: any, data: any[][]) {",
            "    xxx = getLowestLevelIds(",
            "        data.map((e: any[]) => [e[e.length - 1]]),",
            "        'children'",
            "    )",
            "}"
        ],
        "description": "获取树数据结构最低级值"
    },
